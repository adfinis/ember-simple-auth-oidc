{
  "name": "ember-simple-auth-oidc",
  "version": "6.0.0",
  "description": "A Ember Simple Auth addon which implements the OpenID Connect Authorization Code Flow.",
  "keywords": [
    "ember-addon"
  ],
  "license": "LGPL-3.0-or-later",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/adfinis/ember-simple-auth-oidc",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@embroider/macros": "^1.11.1",
    "base64-js": "^1.5.1",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-concurrency": "^3.0.0",
    "ember-fetch": "^8.1.2",
    "ember-simple-auth": "^6.0.0",
    "js-sha256": "^0.9.0",
    "tracked-built-ins": "^3.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@adfinis/eslint-config": "2.0.0",
    "@adfinis/semantic-release-config": "4.0.0",
    "@babel/eslint-parser": "7.22.11",
    "@babel/plugin-proposal-decorators": "7.22.10",
    "@ember/optional-features": "2.0.0",
    "@ember/string": "3.1.1",
    "@ember/test-helpers": "3.1.0",
    "@embroider/test-setup": "3.0.1",
    "@glimmer/tracking": "1.1.2",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "8.2.1",
    "ember-apollo-client": "4.1.1",
    "ember-cli": "5.0.0",
    "ember-cli-clean-css": "2.0.0",
    "ember-cli-dependency-checker": "3.3.2",
    "ember-cli-htmlbars": "6.2.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-mirage": "3.0.0-alpha.3",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-data": "4.12.0",
    "ember-load-initializers": "2.1.2",
    "ember-qunit": "7.0.0",
    "ember-resolver": "10.1.1",
    "ember-source": "5.0.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "5.10.3",
    "ember-try": "2.0.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-ember": "11.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "16.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "7.3.4",
    "graphql": "16.8.1",
    "graphql-tag": "2.12.6",
    "loader.js": "4.7.0",
    "miragejs": "0.1.47",
    "prettier": "2.8.8",
    "qunit": "2.19.4",
    "qunit-dom": "2.0.0",
    "stylelint": "15.8.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-prettier": "3.0.0",
    "webpack": "5.89.0"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": "ember-simple-auth"
  },
  "release": {
    "extends": "@adfinis/semantic-release-config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
