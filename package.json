{
  "name": "ember-simple-auth-oidc",
  "version": "7.3.1",
  "description": "A Ember Simple Auth addon which implements the OpenID Connect Authorization Code Flow.",
  "keywords": [
    "ember-addon"
  ],
  "license": "LGPL-3.0-or-later",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/adfinis/ember-simple-auth-oidc",
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@embroider/macros": "^1.16.11",
    "base64-js": "^1.5.1",
    "ember-auto-import": "^2.10.0",
    "ember-cli-babel": "^8.2.0",
    "ember-concurrency": "^4.0.2 || ^5.0.0",
    "js-sha256": "^0.11.0",
    "tracked-built-ins": "^3.3.0 || ^4.0.0",
    "tracked-toolbox": "^2.0.0",
    "uuid": "^11.0.5 || ^12.0.0 || ^13.0.0"
  },
  "devDependencies": {
    "@apollo/client": "3.14.0",
    "@adfinis/eslint-config": "3.0.2",
    "@adfinis/semantic-release-config": "5.0.0",
    "@babel/eslint-parser": "7.28.4",
    "@babel/plugin-proposal-decorators": "7.28.0",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@ember/optional-features": "2.2.0",
    "@ember/string": "4.0.1",
    "@ember/test-helpers": "5.3.0",
    "@embroider/macros": "1.19.1",
    "@embroider/test-setup": "4.0.0",
    "@eslint/js": "9.38.0",
    "@glimmer/tracking": "1.1.2",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "9.2.1",
    "ember-apollo-client": "4.1.1",
    "ember-cli": "6.8.0",
    "ember-cli-clean-css": "3.0.0",
    "ember-cli-dependency-checker": "3.3.3",
    "ember-cli-deprecation-workflow": "3.4.0",
    "ember-cli-htmlbars": "6.3.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-mirage": "3.0.4",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-data": "5.8.0",
    "ember-fetch": "8.1.2",
    "ember-load-initializers": "3.0.1",
    "ember-qunit": "9.0.4",
    "ember-resolver": "13.1.1",
    "ember-simple-auth": "8.0.1",
    "ember-source": "6.8.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "7.9.3",
    "ember-try": "4.0.0",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-ember": "12.7.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-qunit": "8.2.5",
    "globals": "16.4.0",
    "graphql": "16.11.0",
    "graphql-tag": "2.12.6",
    "husky": "9.1.7",
    "lint-staged": "16.2.4",
    "loader.js": "4.7.0",
    "miragejs": "0.1.48",
    "prettier": "3.6.2",
    "prettier-plugin-ember-template-tag": "2.1.0",
    "qunit": "2.24.2",
    "qunit-dom": "3.5.0",
    "semantic-release": "25.0.1",
    "stylelint": "16.25.0",
    "stylelint-config-standard": "39.0.1",
    "stylelint-prettier": "5.0.3",
    "webpack": "5.102.1"
  },
  "peerDependencies": {
    "@apollo/client": "^3.13.0",
    "@ember-data/adapter": "~4.12.0 || >= 5.0.0 <= 5.3.11",
    "@ember/test-waiters": ">= 4.1.1",
    "@embroider/macros": ">= 1.0.0",
    "ember-data": "~4.12.0 || >= 5.0.0 <= 5.3.11",
    "ember-simple-auth": ">= 6.0.0",
    "ember-source": ">= 4.12.0"
  },
  "peerDependenciesMeta": {
    "@apollo/client": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": ">= 20.11"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": "ember-simple-auth"
  },
  "release": {
    "extends": "@adfinis/semantic-release-config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.hbs": "ember-template-lint --fix",
    "*.css": "stylelint --fix",
    "*.{json,md}": "prettier --write"
  }
}
